  /**定时器倒计时法
   * 生命周期函数--监听页面初次渲染完成
   */
  onReady: function () {
    var interval = setInterval(function () {
      this.data.CountDown_sec--;
      if (this.data.CountDown_sec <= 0) {
        clearInterval(interval);
        wx.showToast({
           title: "计时结束!",
         });
      }
      this.setData({
        CountDown_sec: this.data.CountDown_sec,
      });
    }.bind(this), 1000);
  },
